    <joint name="g_base_to_joint1" type="fixed">
        <axis xyz="0 0 0" />
        <limit effort="1000.0" lower="0" upper="0" velocity="1" />
        <parent link="g_base" />
        <child link="joint1" />
        <origin xyz="0 0 0" rpy="0 0 0" />
        <dynamics damping="0.1" friction="0.1"/>
    </joint>
    <joint name="joint2_to_joint1" type="revolute">
